{
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "Eazy Search",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/assets/icons/log.png",
          "/index.html",
          "/*.css",
          "/*.js"
        ],
        "urls": [
          "https://fonts.googleapis.com/**",
          "https://cdnjs.cloudflare.com/ajax/libs/mdbootstrap/4.8.8/js/mdb.min.js",
          "https://ucarecdn.com/**"
        ]
      }
    }, {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**",
          "/*.(eot|svg|cur|jpg|png|webp|gif|otf|ttf|woff|woff2|ani)"
        ]
      }
    }
  ],
  "dataGroups":[
    {
      "name": "eazy-search-api",
      "urls": [
        "https://api-housing-services-business.herokuapp.com/api/v1/business/",
        "https://api-housing-services-business.herokuapp.com/api/v1/housing/",
        "https://api-housing-services-business.herokuapp.com/api/v1/services/",
        "https://kit.fontawesome.com/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 200,
        "maxAge": "1h",
        "timeout": "5s"
      }
    }

  ]
}
